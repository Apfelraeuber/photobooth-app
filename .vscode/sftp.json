        "delete": true
    },
    "profiles": {
        "dev": {
            "host": "photobooth",
            "uploadOnSave": true
        },
        "prod": {
            "host": "photobooth3",
            "uploadOnSave": true
        },
        "arducam": {
            "host": "photobooth-arducam",
            "uploadOnSave": true
        }
    },
    "defaultProfile": "dev"
}
