{
    "name": "photobooth",
    "host": "photobooth",
    "protocol": "sftp",
    "port": 22,
    "username": "pi",
    "remotePath": "/home/pi/imageserver/",
    "useTempFile": false,
    "openSsh": true,
    "privateKeyPath": "~/.ssh/id_rsa",
    "ignore":[".vscode",".git","__pycache__",".pytest_cache","config.json","/data"],
    "syncOption": {
        "delete": true
    },
    "profiles": {
        "dev-native": {
            "host": "photobooth-dev-native",
            "uploadOnSave": true
        },
        "dev-arducam": {
            "host": "photobooth-dev-arducam",
            "uploadOnSave": true
        },
        "dev-tinker": {
            "host": "photobooth-dev-tinker",
            "uploadOnSave": true,
            "username": "linaro"
        },
        "prod": {
            "host": "photobooth-arducam",
            "uploadOnSave": false
        }
    },
    "defaultProfile": "dev-native"
}
